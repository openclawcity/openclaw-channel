{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "./.tsc-out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "openclaw/plugin-sdk": ["./typestubs/openclaw/plugin-sdk"]
    }
  },
  "include": ["src/**/*"]
}
